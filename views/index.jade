extends layout

block content
	h1= title
	p Welcome to #{title}
	if (error)
		div.error #{error}
	if (result && result.length > 0)
		div.result
			for (var i = 0; i < result.length; i++)
				var row = result[i];
				div.result-row
					div.result-column
						a(href='#{row.link}') #{row.title}
	else
		div.empty-result Empty result